  "description": "react+redux+axios+antd+webpack",
  "main": "index.js",
  "scripts": {
    "server": "node server",
    "server:mock": "node ./mock/mock-server",
    "dev": "cross-env NODE_ENV=development ENV=dev webpack-dev-server --mode development --config build/webpack.config.js --hot",
    "dev:mock": "cross-env NODE_ENV=development ENV=mock webpack-dev-server --mode development --config build/webpack.config.js --hot",
    "test": "cross-env NODE_ENV=production ENV=test webpack --progress --mode production --config build/webpack.config.js",
    "build": "cross-env NODE_ENV=production ENV=prod webpack --progress --mode production --config build/webpack.config.js",
    "dll:dev": "cross-env NODE_ENV=development webpack --mode development --progress --config build/webpack.dll.config.js",
    "dll:prod": "cross-env NODE_ENV=production webpack --mode production --progress --config build/webpack.dll.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lorin19881101/react-redux-axios-antd-webpack.git"
  },
